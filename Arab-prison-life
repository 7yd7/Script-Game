  end
end

end
end)


-- قتل لما يلمس لاعب حجم معين
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".killaura"
if string.sub(message, 1, #keyword) == keyword then

getgenv().Kill_Aura = true

end
end)

-- ايقاف قتل لما يلمس لاعب حجم معين 
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".unkillaura"
if string.sub(message, 1, #keyword) == keyword then

getgenv().Kill_Aura = false

end
end)

-- loop

-- تكرار قتل جميع لاعبين بل ماب 
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".loopkill all"
if string.sub(message, 1, #keyword) == keyword then

  getgenv().Killall = true

  game.StarterGui:SetCore("SendNotification",  {
    Title = "تم تشغيل قتل جميع لاعبين";
    Text = "By : 7yd7";
    Icon = "";
    Duration = 5;
    Callback = NotificationBindable;
   })

end
end)

