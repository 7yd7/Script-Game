-- سكربتات من نوع loop

-- قتل من مسافه بعيد
coroutine.wrap(function()
while wait() do
  if getgenv().Kill_Aura == true then
    for i, v in pairs(game.Players:GetPlayers()) do
      if v ~= game.Players.LocalPlayer then
        local Distance = (v.Character:FindFirstChildOfClass("Part").Position - game.Players.LocalPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
        if Distance <= 10 then
          for i = 1, 25 do
            local ohInstance1 = v.Character.Humanoid
            local ohNumber2 = 1000000000000

            if v.Character.Humanoid.Health > 0 then

            game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)
          end
        end
      end
    end
  end
end
end
end)()

coroutine.wrap(function()
  while wait(.5) do
    if getgenv().Killall == true then
      for i, v in pairs(game.Players:GetPlayers()) do
        if v.Name ~= game.Players.LocalPlayer.Name then
            local ohInstance1 = v.Character.Humanoid
            local ohNumber2 = 1000000000000
    
            if v.Character.Humanoid.Health > 0 then
                game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)
            end
        end
    end
    end
  end
  end)()

  coroutine.wrap(function()
    while wait(.5) do
      if getgenv().killInm == true then
        for i,v in pairs(game:GetService("Teams").Inmates:GetPlayers()) do
          if v.Name ~= game.Players.LocalPlayer.Name then
              local ohInstance1 = v.Character.Humanoid
              local ohNumber2 = 1000000000000
      
              if v.Character.Humanoid.Health > 0 then
                  game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)
              end
          end
      end
      end
    end
    end)()

    coroutine.wrap(function()
      while wait(.5) do
        if getgenv().killGua == true then
          for i,v in pairs(game:GetService("Teams").Guards:GetPlayers()) do
            if v.Name ~= game.Players.LocalPlayer.Name then
                local ohInstance1 = v.Character.Humanoid
                local ohNumber2 = 1000000000000
        
                if v.Character.Humanoid.Health > 0 then
                    game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)
                end
            end
        end
        end
      end
      end)()

      coroutine.wrap(function()
        while wait(.5) do
          if getgenv().killCri == true then
            for i,v in pairs(game:GetService("Teams").Criminals:GetPlayers()) do
              if v.Name ~= game.Players.LocalPlayer.Name then
                  local ohInstance1 = v.Character.Humanoid
                  local ohNumber2 = 1000000000000
          
                  if v.Character.Humanoid.Health > 0 then
                      game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)
                  end
              end
          end
          end
        end
        end)()

        coroutine.wrap(function()
          while wait(.5) do
            if getgenv().arrestall == true then
              for i,v in pairs(game.Players:GetPlayers()) do
                if v.Name ~= game.Players.LocalPlayer.Name then     
            local ohInstance1 = v
            
            game:GetService("ReplicatedStorage").Arrest:InvokeServer(ohInstance1)
                end
            end
            end
          end
          end)()

