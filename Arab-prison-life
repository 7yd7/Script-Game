

local function sendSystemMessage()
  game.StarterGui:SetCore("ChatMakeSystemMessage", {
  Text = "[7yd7] : .cmds معرفه جميع اوامر"; -- النص سكربت يلي يكتبه شات فقط يبان عندك
      Font = Enum.Font; --font
      Color = Color3.new(252, 250, 255); -- الون شات حاليا لونه ابيض
      FontSize = Enum.FontSize.Size96 --size
  })
end

sendSystemMessage()

