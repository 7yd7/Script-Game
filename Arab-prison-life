-- سكربتات

-- تحديد اسم لاعب
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".kill "
if string.sub(message, 1, #keyword) == keyword then
  local Name = string.sub(message, #keyword + 1)
  local players = game:GetService("Players")
  for _, player in ipairs(players:GetPlayers()) do
    if string.sub(player.Name, 1, #Name) == Name then
      v = player.Name
      break
    end
  end

  local ohInstance1 = game.Players[v].Character.Humanoid
  local ohNumber2 = 1000000000000
  
  game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)

end
end)

-- قتل جميع لاعبين بل ماب
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".kill all"
if string.sub(message, 1, #keyword) == keyword then

for i,v in pairs(game.Players:GetPlayers()) do
  if v.Name ~= game.Players.LocalPlayer.Name then

local ohInstance1 = v.Character.Humanoid
local ohNumber2 = 1000000000000

game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)

  end
end

end
end)

-- قتل لاعبين بل تيم حرامي
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".kill inm"
if string.sub(message, 1, #keyword) == keyword then

for i,v in pairs(game:GetService("Teams").Inmates:GetPlayers()) do
  if v ~= game.Players.LocalPlayer then

local ohInstance1 = v.Character.Humanoid
local ohNumber2 = 1000000000000

game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)

  end
end

end
end)

-- قتل لاعبين بل تيم شرطي
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".kill gua"
if string.sub(message, 1, #keyword) == keyword then

for i,v in pairs(game:GetService("Teams").Guards:GetPlayers()) do
  if v ~= game.Players.LocalPlayer then

local ohInstance1 = v.Character.Humanoid
local ohNumber2 = 1000000000000

game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)

  end
end

end
end)

-- قتل لاعبين بل تيم مجرم
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".kill cri"
if string.sub(message, 1, #keyword) == keyword then

for i,v in pairs(game:GetService("Teams").Criminals:GetPlayers()) do
  if v ~= game.Players.LocalPlayer then

