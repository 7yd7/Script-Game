-- سكربتات

-- تحديد اسم لاعب
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
  local keyword = ".kill "
  if string.sub(message, 1, #keyword) == keyword then
    local Name = string.sub(message, #keyword + 1)
    local players = game:GetService("Players")
    for _, player in ipairs(players:GetPlayers()) do
      if string.sub(player.Name, 1, #Name) == Name then
        v = player.Name
        break
      end
    end

    local ohInstance1 = game.Players[v].Character.Humanoid
    local ohNumber2 = 1000000000000
    
    game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)

  end
end)

-- قتل جميع لاعبين بل ماب
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".kill all"
if string.sub(message, 1, #keyword) == keyword then

for i,v in pairs(game.Players:GetPlayers()) do
    if v.Name ~= game.Players.LocalPlayer.Name then

local ohInstance1 = v.Character.Humanoid
local ohNumber2 = 1000000000000

game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)

    end
end

  end
end)

-- ما تموت
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".godmode"
if string.sub(message, 1, #keyword) == keyword then

    local ohInstance1 = game.Players.LocalPlayer.Character.Humanoid
    local ohNumber2 = -math.huge
    
    game:GetService("ReplicatedStorage").GunRemote.Tag:FireServer(ohInstance1, ohNumber2)    

  end
end)


-- اخذ جميع اغراض
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".getalltools"
if string.sub(message, 1, #keyword) == keyword then

    fireclickdetector(workspace.Prison_ITEMS.giver["AK-47"].ClickDetector)
    fireclickdetector(workspace.Prison_ITEMS.giver.FAL.ClickDetector)
    fireclickdetector(workspace.Prison_ITEMS.giver.M9.ClickDetector)
    fireclickdetector(workspace.Prison_ITEMS.giver.MP5.ClickDetector)
    fireclickdetector(workspace.Prison_ITEMS.giver.MiniGun.ClickDetector)
    fireclickdetector(workspace.Prison_ITEMS.giver["Remington 870"].ClickDetector)
    
    
  end
end)

-- رصاص لا محدود
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".infammos"
if string.sub(message, 1, #keyword) == keyword then

for _, infAmmos in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do

        infAmmos.Ammos.Value = math.huge
end
    
  end
end)

-- سجن جميع لاعبين ( مخالفين او طالعين برا سجن )
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".arrest all"
if string.sub(message, 1, #keyword) == keyword then


    for i,v in pairs(game.Players:GetPlayers()) do
        if v.Name ~= game.Players.LocalPlayer.Name then
    
    local ohInstance1 = v
    
    game:GetService("ReplicatedStorage").Arrest:InvokeServer(ohInstance1)
    
        end
    end

  end
end)

-- حذف جميع ابواب
local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".deldoor"
if string.sub(message, 1, #keyword) == keyword then

    local Doors = workspace.Doors:GetChildren()

    if #Doors == 0 then
game.StarterGui:SetCore("SendNotification",  {
 Title = "انه محذوف بل فعل";
 Text = "by : 7yd7";
 Icon = "";
 Duration = 5;
 Callback = NotificationBindable;
})
    else
        game.StarterGui:SetCore("SendNotification",  {
            Title = "يتم حذف جميع ابواب";
            Text = "by : 7yd7";
            Icon = "";
            Duration = 5;
            Callback = NotificationBindable;
           })
      for _, Doors in pairs(Doors) do
        Doors:Destroy()
      end
    end
    
  end
end)
