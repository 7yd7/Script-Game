local function sendSystemMessage()
    game.StarterGui:SetCore("ChatMakeSystemMessage", {
    Text = "[7yd7] : .cmde معرفه جميع اوامر"; -- النص سكربت يلي يكتبه شات فقط يبان عندك
        Font = Enum.Font; --font
        Color = Color3.new(252, 250, 255); -- الون شات حاليا لونه ابيض
        FontSize = Enum.FontSize.Size96 --size
    })
end

sendSystemMessage()

local player = game.Players.LocalPlayer
local chattedConnection = player.Chatted:Connect(function(message)
local keyword = ".cmde"
if string.sub(message, 1, #keyword) == keyword then

    local function sendSystemMessage()
        game.StarterGui:SetCore("ChatMakeSystemMessage", {
        Text = "[7yd7] : .kill قتل حط اسم لاعب"; -- النص سكربت يلي يكتبه شات فقط يبان عندك
            Font = Enum.Font; --font
            Color = Color3.new(252, 250, 255); -- الون شات حاليا لونه ابيض
            FontSize = Enum.FontSize.Size96 --size
        })
    end
    
    sendSystemMessage()

    local function sendSystemMessage()
        game.StarterGui:SetCore("ChatMakeSystemMessage", {
        Text = "[7yd7] : .kill all قتل جميع لاعبين"  ; 
            Font = Enum.Font; --font
            Color = Color3.new(252, 250, 255); -- الون شات حاليا لونه ابيض
            FontSize = Enum.FontSize.Size96 --size
        })
    end
    
    sendSystemMessage()

    local function sendSystemMessage()
        game.StarterGui:SetCore("ChatMakeSystemMessage", {
        Text = "[7yd7] : .godmode ما تموت"  ; 
            Font = Enum.Font; --font
            Color = Color3.new(252, 250, 255); -- الون شات حاليا لونه ابيض
            FontSize = Enum.FontSize.Size96 --size
        })
    end
    
    sendSystemMessage()

    local function sendSystemMessage()
        game.StarterGui:SetCore("ChatMakeSystemMessage", {
        Text = "[7yd7] : .getalltools اخذ جميع اغراض (تقريبا)"  ; 
            Font = Enum.Font; --font
            Color = Color3.new(252, 250, 255); -- الون شات حاليا لونه ابيض
            FontSize = Enum.FontSize.Size96 --size
        })
    end
    
    sendSystemMessage()

    local function sendSystemMessage()
        game.StarterGui:SetCore("ChatMakeSystemMessage", {
        Text = "[7yd7] : .infammos (لا تمسك الغرض) رصاص لا محدود"  ; 
            Font = Enum.Font; --font
            Color = Color3.new(252, 250, 255); -- الون شات حاليا لونه ابيض
            FontSize = Enum.FontSize.Size96 --size
        })
    end
    
