local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("تجمع مشاهير", "BloodTheme")

local gTab = Window:NewTab("hack")
local tTab = Window:NewTab("tools")
local zTab = Window:NewTab("Settings")


local Section = gTab:NewSection("hack")


Section:NewButton("voteKickMenu", "ButtonInfo", function()
game:GetService("Players").LocalPlayer.PlayerGui.voteKickMenu.openclose.Visible = true
game:GetService("Players").LocalPlayer.PlayerGui.voteKickMenu.openclose.Position = UDim2.new(0.9203282, 0, 0.213526264, 0)
game:GetService("Players").LocalPlayer.PlayerGui.voteKickMenu.Timer.Visible = true
game:GetService("Players").LocalPlayer.PlayerGui.voteKickMenu.Timer.Position = UDim2.new(0.919695795, 0, 0.11279849, 0)
end)

Section:NewButton("NukeGui", "ButtonInfo", function()
game:GetService("Players").LocalPlayer.PlayerGui.NukeGui:Destroy()
wait(.1)
game:GetService("Players").LocalPlayer.PlayerGui.NukeGui.OpenButtonBase.Visible = true


end)

Section:NewToggle("MP5K Activate", "", function(state)
    if state then
        getgenv().MP5K = true 
        repeat wait ()
-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

workspace.afwafwwgwe22.MP5K.ActivateSpecial:FireServer()
        until getgenv().MP5K == false
    else
        getgenv().MP5K = false
    end
end)

Section:NewToggle("Give Money all", "", function(state)
    if state then
        getgenv().MP5K = true 
        repeat wait ()
for i,v in pairs(game.Players:GetPlayers()) do
    if v ~= game.Players.LocalPlayer then
local ohInstance1 = v
local ohNumber2 = -999999999999999999999999999999999999999999999999

game:GetService("ReplicatedStorage").GiveMoney:FireServer(ohInstance1, ohNumber2)
	end
end
        until getgenv().MP5K == false
    else
        getgenv().MP5K = false
    end
end)


local Section = tTab:NewSection("tools")


Section:NewButton("Nuke", "ButtonInfo", function()
-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

local ohString1 = "Tool3"

game:GetService("Players").LocalPlayer.PlayerGui.NukeGui.CodeFrame.extraframe.RemoteEvent:FireServer(ohString1)
end)


Section:NewButton("Mp5k", "ButtonInfo", function()
-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

local ohString1 = "Tool2"

game:GetService("Players").LocalPlayer.PlayerGui.NukeGui.CodeFrame.extraframe.RemoteEvent:FireServer(ohString1)

end)

Section:NewButton("Pistol", "ButtonInfo", function()
-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

local ohString1 = "Tool1"

game:GetService("Players").LocalPlayer.PlayerGui.NukeGui.CodeFrame.extraframe.RemoteEvent:FireServer(ohString1)

end)



local Section = zTab:NewSection("Settings")

Section:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.G, function()
	Library:ToggleUI()
end)
