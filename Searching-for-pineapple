-- تاكيد اذا لديك اغراض او لا
local Backpack = game:GetService("Players").LocalPlayer.Backpack

local count = 0

for _, item in ipairs(Backpack:GetChildren()) do
  count = count + 1
end

if count >= 6 then
    game.StarterGui:SetCore("SendNotification",  {
        Title = "يبدو لديك جميع اغراض قيم باس";
        Text = "By : 7yd7";
        Icon = "";
        Duration = 5;
        Callback = NotificationBindable;
       })
else
-- قيم باس
game:GetService("ReplicatedStorage").SuperFlyGoldBoombox:FireServer()
game:GetService("ReplicatedStorage").SpeedCoil:FireServer()
game:GetService("ReplicatedStorage").GravityCoil:FireServer()
game:GetService("ReplicatedStorage").FidgetSpinner:FireServer()
game:GetService("ReplicatedStorage").BodySwapPotion:FireServer()
game:GetService("ReplicatedStorage").Hoverboard:FireServer()
-- رساله قيم باس
game.StarterGui:SetCore("SendNotification",  {
    Title = "تم اخذ جميع الاغراض قيم باس";
    Text = "(لما تطلع تدخل سوف يذهب قيم باس)";
    Icon = "";
    Duration = 5;
    Callback = NotificationBindable;
   })
end

-- حد اقصى 50 حاليا
   local leaderstats = game:GetService("Players").LocalPlayer.leaderstats

   local value = leaderstats["\216\167\217\134\216\167\217\134\216\167\216\179"].Value
   
if value >= 50 then
   -- لديك جميع اناناس
   game.StarterGui:SetCore("SendNotification",  {
    Title = "يبدو لديك جميع Badge/اناناس";
    Text = "By : 7yd7";
    Icon = "";
    Duration = 5;
    Callback = NotificationBindable;
   })
else

-- الانتظار
game.StarterGui:SetCore("SendNotification",  {
 Title = "سوف يتم اخذ جميع badges/الاناناس";
 Text = "(يستغرق وقتا سوف انبه لك لما تحصل جميع badges/الاناناس)";
 Icon = "";
 Duration = 5;
 Callback = NotificationBindable;
})

-- اساس ماب
local workspacef = workspace
local foundCar = false

local badges = {}

for _, child in pairs(workspacef:GetChildren()) do
  if child.Name:match("Badge") then
    badges[#badges + 1] = child
  end
end

for _, badge in ipairs(badges) do
  local success, error = pcall(function()
    local part = badge

    local originalCFrame = part.CFrame

    part.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

    wait(.2)

    part.CFrame = originalCFrame
  end)
end

wait(.3)
