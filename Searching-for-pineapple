-- حصول اناناسة

game.StarterGui:SetCore("SendNotification",  {
    Title = "يتم الان حصول علي اناناس";
    Text = "By : 7yd7";
    Icon = "";
    Duration = 5;
    Callback = NotificationBindable;
   })

   -- سكربت

   local workspacef = workspace.ToFind
local foundCar = false

local Pineapple = {}

for _, child in pairs(workspacef:GetChildren()) do
    Pineapple[#Pineapple + 1] = child
  end


for _, Pineapple in ipairs(Pineapple) do
  local success, error = pcall(function()
    local part = Pineapple

    local originalCFrame = part.CFrame

    part.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

    wait(.1)

    part.CFrame = originalCFrame
  end)
end

local player = game.Players.LocalPlayer
local rootPart = player.Character.HumanoidRootPart

local lastPosition = rootPart.CFrame.Position
local lastRotation = rootPart.CFrame.Rotation
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(548.955261, 66.4984245, 780.494263, -0.707134247, 0, 0.707079291, 0, 1, 0, -0.707079291, 0, -0.707134247)

wait(.5)
rootPart.CFrame = CFrame.fromEulerAngles(lastRotation.X, lastRotation.Y, lastRotation.Z) * CFrame.new(lastPosition)

-- تم حصول جميع انانس

game.StarterGui:SetCore("SendNotification",  {
    Title = "تم حصول جميع اناناس";
    Text = "By : 7yd7";
    Icon = "";
    Duration = 5;
    Callback = NotificationBindable;
   })
end
